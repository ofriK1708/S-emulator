<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" stylesheets="@RunControls.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.jfx.runControls.RunControlsController">
   <children>
      <Button fx:id="setRun" mnemonicParsing="false" onAction="#onSetPress" text="set new run  ⚙️" HBox.hgrow="ALWAYS">
         <styleClass>
            <String fx:value="pill-button" />
            <String fx:value="pill-grey" />
         </styleClass>
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="run" mnemonicParsing="false" onAction="#onRunPressed" text="▶️" HBox.hgrow="ALWAYS">
         <styleClass>
            <String fx:value="pill-button" />
            <String fx:value="pill-blue" />
         </styleClass>
         <tooltip>
            <Tooltip text="run" />
         </tooltip>
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="debug" mnemonicParsing="false" onAction="#onDebugPressed" text="🪲" HBox.hgrow="ALWAYS">
         <styleClass>
            <String fx:value="pill-button" />
            <String fx:value="pill-green" />
         </styleClass>
         <tooltip>
            <Tooltip text="debug" />
         </tooltip>
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="6.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</HBox>
