<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox spacing="10.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.jfx.debugger.DebuggerController">
   <children>
      <HBox prefHeight="26.0" prefWidth="129.0">
         <children>
            <FlowPane hgap="40.0" vgap="10.0" HBox.hgrow="NEVER">
               <children>
               
                                          <!-- Row 2 -->
                     <Button fx:id="stop" text="Stop" onAction="#handleStop">
                     <FlowPane.margin>
                        <Insets left="15.0" />
                     </FlowPane.margin>
                  </Button>
                     <Button fx:id="resume" text="resume" onAction="#handleResume" />
               </children>
            </FlowPane>
         </children>
      </HBox>
      <HBox prefHeight="26.0" prefWidth="216.0">
         <children>
            <FlowPane hgap="10.0" prefHeight="4.0" prefWidth="208.0" prefWrapLength="300.0" vgap="10.0">
               <children>
               
                                          <!-- Row 3 -->
                     <Button fx:id="stepOver" text="Step Over" onAction="#handleStepOver"/>
                     <Button fx:id="stepBackward" text="Step Backward" onAction="#handleStepBackward"/>
               </children>
            </FlowPane>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
