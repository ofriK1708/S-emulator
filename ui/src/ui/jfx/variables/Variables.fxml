<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ui.jfx.variables.VariablesController" spacing="8.0">
    <children>
        <TableView fx:id="variablesTable" VBox.vgrow="ALWAYS" prefHeight="120.0">
            <columns>
                <TableColumn fx:id="variableColumn" text="Arguments(X)" prefWidth="120.0" />
                <TableColumn fx:id="valueColumn" text="Value" prefWidth="80.0" />
            </columns>
            <placeholder>
                <Label text="No variables to display" />
            </placeholder>
        </TableView>
    </children>
</VBox>
